<div class="container">
  <div class="row">
   <div class="col-md-9 ">
    <h4>ジャンル編集</h4>
    <%= form_with model: @genre, url: '/admin/genres' do |f| %>
    
       <div class="form-group">
        <%= f.label :"ジャンル名" %>
        <%= f.text_field :name, class: "form-control" %>
     
     　</div>
      
     
        <div class="form-group">
        <%= f.submit '変更を保存', class: "btn btn-success" %>
        </div>
     
      
    <% end %>
    